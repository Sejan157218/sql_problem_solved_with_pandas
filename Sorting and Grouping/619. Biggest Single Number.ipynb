   "source": [
    "import pandas as pd\n",
    "\n",
    "def biggest_single_number(my_numbers: pd.DataFrame) -> pd.DataFrame:\n",
    "    my_numbers['duplicated'] = my_numbers.duplicated(keep=False)\n",
    "    df = my_numbers[my_numbers['duplicated']==False]\n",
    "    return pd.DataFrame([df['num'].max()], columns=['num'])"
   ]
  }
