    "import pandas as pd\n",
    "\n",
    "def find_classes(courses: pd.DataFrame) -> pd.DataFrame:\n",
    "    df = courses['class'].value_counts().loc[lambda x : x >= 5].reset_index()\n",
    "    return df[['class']]\n",
    "    "
