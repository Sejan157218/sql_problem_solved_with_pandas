   "source": [
    "import pandas as pd\n",
    "\n",
    "def consecutive_numbers(logs: pd.DataFrame) -> pd.DataFrame:\n",
    "    df = logs[(logs.num.diff() == 0) & (logs.num.diff().diff() == 0)]\n",
    "    return df[['num']].drop_duplicates().rename(columns={'num': 'ConsecutiveNums'})\n",
    "    "
   ]
  }
