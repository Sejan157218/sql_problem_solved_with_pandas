   "source": [
    "import pandas as pd\n",
    "\n",
    "def replace_employee_id(employees: pd.DataFrame, employee_uni: pd.DataFrame) -> pd.DataFrame:\n",
    "    df  = employees.merge(employee_uni, on=\"id\", how=\"outer\")[['unique_id','name']]\n",
    "    return df[df.name.notnull()] "
   ]
